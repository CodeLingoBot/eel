    "IsTransformationByExample": false,
    "Path": "",
    "Verb": "POST",
    "Endpoint": "",
    "HttpHeaders": {
      "X-B3-TraceId": "{{traceid()}}",
      "X-Tenant-Id": "{{tenant()}}"
    }
}
