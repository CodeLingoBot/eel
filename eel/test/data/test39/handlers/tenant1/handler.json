{
	"Version": "1.0",
	"Name": "MatchPartialArrays2",
	"Info": "Filter array if event is missing any of the values.",
	"Active": true,
	"Match": {
		"content": {
			"device": "sensor02",
			"temperature": "42",
			"recent": [40, 41, 51]
    }
	},
	"IsMatchByExample": true,
	"TerminateOnMatch": true,
	"Transformation": {
		"{{/}}": "{{/}}"
	},
	"IsTransformationByExample": false,
	"Path": "",
	"Verb": "POST",
	"Endpoint": "",
	"HttpHeaders": {
		"X-B3-TraceId": "{{traceid()}}",
		"X-Tenant-Id": "{{tenant()}}"
	}
}
